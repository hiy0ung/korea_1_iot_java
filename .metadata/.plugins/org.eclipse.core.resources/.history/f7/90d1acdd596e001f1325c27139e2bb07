package chapter09;

interface Flyable {
	// 상수 final 생략 (public static final)
	// >> UPPER_SNAKE_CASE 사용 권장
	// >> 선언과 동시에 초기화
	int MAX_SPEED = 100;
	
	void fly(); // (public abstract 생략)
	
	// 디폴트 메서드 >> 재정의 가능
	default void land() {
		System.out.println("Landing");
	}
	
	// 정적 메서드 >> 재정의 불가능
	static int getWingCount() {
		return 2;
	}
}

public class C_Interface {

}
