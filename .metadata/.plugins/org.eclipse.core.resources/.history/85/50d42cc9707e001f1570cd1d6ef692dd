package java_practice;

class Animal {
	String name;
	
	public void act() {
		System.out.println("동물이 소리를 냅니다.");
	}
}

class Dog extends Animal {
	String dogName;
	
	@Override
	public void act() {
		super.act();
		System.out.println("강아지");
	}
}

class Cat extends Animal {
	String catName;
	
	@Override
	public void act() {
		super.act();
		System.out.println("고양이");
	}
}


public class Q9 {
	public static void main(String[] args) {
		Dog dog = new Dog();
		dog.act();
	}
}
